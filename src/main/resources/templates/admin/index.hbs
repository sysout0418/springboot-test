    <div id="layoutSidenav_content">
        <main>
            <div class="container-fluid">
                <h1 class="mt-4">File</h1>
                <ol class="breadcrumb mb-4">
                    <li class="breadcrumb-item active"><a href="/admin/index">File</a></li>
                </ol>
                <div class="card mb-4">
                    <div class="card-body">파일 업로드</div>
                </div>
                <div class="card mb-4">
                    <div class="card-header"><i class="fas fa-table mr-1"></i>
                        파일 목록
                        <div class="input-group-append">
                            <a class="btn btn-primary" href="/admin/upload" role="button">업로드</a>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                <thead>
                                <tr>
                                    <th>번호</th>
                                    <th>제목</th>
                                    <th>파일명</th>
                                    <th>파일타입</th>
                                    <th>파일사이즈</th>
                                    <th>등록자</th>
                                    <th>등록일</th>
                                </tr>
                                </thead>
                                <tbody>
                                {{#each fileList}}
                                <tr>
                                    <td>{{fileNo}}</td>
                                    <td>{{title}}</td>
                                    <td>{{fileName}}</td>
                                    <td>{{fileType}}</td>
                                    <td>{{fileSize}}</td>
                                    <td>{{insertedId}}</td>
                                    <td>{{insertedDate}}</td>
                                </tr>
                                {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </main>
